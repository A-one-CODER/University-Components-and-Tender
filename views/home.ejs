    <section class="services">
        <h1>Our Services</h1>
        <div class="container">
            <div class="row">
                <div class="cold-md-3 fetdiv">
                    <i class="fa-solid fa-bell fa-3x"></i>
                    <h4>Tender Alert</h4>
